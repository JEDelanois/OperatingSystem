#define barLength 320

/*
This function takes a null terminated string of characters and trans forms it 
into a graphical image on the status bar. If it is greater than 40 chars or error 
then it returns -1. If it is sucessful it will center the text and print it to the 
status bar. background is the background color and tcolor is the text color.
inputs:
	string to be printed
	status bar to be updated
	the background color
	the text color
outputs: 0 if sucessful -1 if error
sideaffects: the the status bar passed to this funtions will contain the pixel image
of the string passed to it
*/
int text_to_graphics(char * string, unsigned char * bar,
 unsigned char background, unsigned char tcolor){
	int i = 0;
	//first find how many characters are in the string
	int nchars = 0;
	for( i = 0; string[i] != '\0'; i++){
		nchars++;
	}
	
	//if too many chars return -1 for error
	if(nchars > 40)
		return -1;
		
	//find offset needed to center text
	// (total line - space by chars)/2 is how far you need to push each line over
	int t_offset = (320-(nchars*8))/2;
	unsigned char temp[5760];
	
	// this set of for loops maps the text to the temp array in normal text mode
	int r,c;
	for( r = 0; r < 18; r++){	
		for( c = 0; c < 320; c++){
			if(r == 0)
				temp[(r*barLength) + c] = background; // color above text background
			else if( r == 17)
				temp[(r*barLength) + c] = background;// color below text background
			else if(c < t_offset)
				temp[(r*barLength) + c] = background;// backe left text background
			else if( c >= t_offset + (8*nchars))
				temp[(r*barLength) + c] = background;// right of text background
			else{
				//get current char index
				int cur = (c - t_offset)/8;
				
				//get current pixel bit
				char line = font_data[(int)string[cur]][r-1];//account for extra rows
				
				//find which bit you  want in the row
				// colums - offset - how far down down th eline wiht chars
				int bit_number = c - (t_offset) - (cur*8);
				
				//can i do this??
				//bool text = 1 & (line >> (7 - bit_number));// find if text is 
															//present here
				if(1 & (line >> (7 - bit_number)))
					temp[(r*barLength) + c] = tcolor;
				else	
					temp[(r*barLength) + c] = background;
					
			}
			
	
		}
	}
 
	//this par of the code maps the linear array temp into the status bar
	// buffer in plane format starting with 3,2,1,0
	
	//maps plane 3
	for( i = 0; i < 1440; i++){
		bar[i] = temp[(4*(i+1)) - 1];
	}
	
	
	//map to plane2
	for( i = 0; i < 1440;i++){
		bar[i +1440] = temp[(4*(i+1)) - 2];
	}
	
	//map to plane1
	for( i = 0; i < 1440;i++){
		bar[i +2880] = temp[(4*(i+1)) - 3];
	}
	
	//map to plane 0
	for( i = 0; i < 1440;i++){
		bar[i +4320] = temp[(4*(i+1)) -4];
	}
	return 0;
 }

 
 /*
translates a string of fruit into the graphics pixels array text to graphics
Inputs:fruit - array of character to be pixalated
		array - array of repreentative pixes
Outputs: returns 0

Side-Affects:
populates array with pixel representation
*/
 int fruit_to_graphics(char * fruit, unsigned char * array){
	int r,c,i;
	char chrow;
	
	if( fruit == NULL)
		return -1;
	
	//for all the pixels
	for(r = 0; r < 16; r++){
		for(c = 0; c < 104;c++){
			chrow = font_data[((unsigned int)fruit[c/8])][r];
			
			// copy row of pixes into buffer
			for(i = 0; i<8; i++){
			
				// copy pixel if there is one that exist
				if( (chrow >> (7 - i)) & 1)
					array[104*r + c + i] = 0x1;
				else
					array[104*r + c + i] = 0x0;
			}		
			//go to next chunk
			c += 7;

		
		}
	}
	return 0;
 }
 
 
 
 
 
 
 
 
 
 
 
